{
  "_version": "1.28.0",
  "sap.app": {
    "id": "com.solvia.management",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.14.0",
      "toolsId": "12f74f03-3111-45ce-b7ec-d69082d7badb"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZTK_APP_SERVICE_SRV_01/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.84.48",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.solvia.management.i18n.i18n"
        }
      },
      "myOdata": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.solvia.management.view",
        "controlAggregation": "pages",
        "controlId": "idApp",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteMain",
          "pattern": ":?query:",
          "target": ["TargetMain"]
        },
        {
          "pattern": "addProduct",
          "name": "addProduct",
          "target": "addProduct"
        },
        {
          "pattern": "addEmployee",
          "name": "addEmployee",
          "target": "addEmployee"
        },
        {
          "pattern": "addAddress",
          "name": "addAddress",
          "target": "addAddress"
        }
      ],
      "targets": {
        "TargetMain": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Main",
          "viewName": "Main"
        },
        "addProduct": {
          "viewName": "addProduct"
        },
        "addEmployee": {
          "viewName": "addEmployee"
        },
        "addAddress": {
          "viewName": "addAddress"
        }
      }
    },
    "rootView": {
      "viewName": "com.solvia.management.view.App",
      "type": "XML",
      "async": true,
      "id": "idApp"
    }
  }
}
